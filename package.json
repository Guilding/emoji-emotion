{
  "name": "emoji-emotion",
  "version": "0.1.0",
  "description": "List of emoji rated for valence in JSON",
  "license": "MIT",
  "keywords": [
    "gemoji",
    "emoji",
    "unicode",
    "word",
    "list",
    "sentiment",
    "analysis",
    "opinion",
    "mining",
    "text",
    "microblogs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/emoji-emotion.git"
  },
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "devDependencies": {
    "browserify": "^8.0.0",
    "eslint": "^0.13.0",
    "esmangle": "^1.0.0",
    "gemoji": "^0.2.0",
    "istanbul": "^0.3.0",
    "jscs": "^1.0.0",
    "jscs-jsdoc": "^0.4.0",
    "markdown-table": "^0.3.0",
    "mocha": "^2.0.0",
    "plain-text-data-to-json": "^0.1.0"
  },
  "scripts": {
    "test": "_mocha --check-leaks test.js",
    "test-travis": "istanbul cover _mocha --report lcovonly -- --check-leaks test.js",
    "coverage": "istanbul cover _mocha -- test.js",
    "lint-api": "eslint index.js",
    "lint-test": "eslint --env mocha test.js",
    "lint-script": "eslint script/build-data.js script/build-support.js",
    "lint-style": "jscs --reporter inline index.js script/build-data.js script/build-support.js test.js",
    "lint": "npm run lint-api && npm run lint-test && npm run lint-style",
    "make": "npm run lint && npm run coverage",
    "build-data": "node script/build-data.js",
    "build-support": "node script/build-support.js",
    "build": "npm run build-data && npm run build-support",
    "prepublish": "npm run build"
  }
}
